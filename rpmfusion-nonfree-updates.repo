[rpmfusion-nonfree-updates]
name=RPM Fusion for EL 10 - Nonfree - Updates
#baseurl=http://download1.rpmfusion.org/nonfree/el/updates/10/$basearch/
metalink=http://mirrors.rpmfusion.org/metalink?repo=nonfree-el-updates-released-10&arch=$basearch
enabled=1
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-nonfree-el-10

